{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python@3.1.32",
      "config": {
        "runtime": "python3.9",
        "installCommand": "pip install --no-cache-dir --upgrade pip && pip install --no-cache-dir -r requirements.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ]
}
