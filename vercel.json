{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "pythonVersion": "3.12",
        "includeFiles": [
          "main.py",
          "requirements.txt",
          "runtime.txt",
          "templates/**",
          "static/**",
          "inference.py",
          "crypto.py",
          "model.json"
        ],
        "excludeFiles": [
          "**/*.ipynb",
          "hardversion.py",
          "extract_random_forest.py",
          "examples.html",
          "model.pkl",
          ".git/**",
          "README.md"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/main.py"
    }
  ]
}
